{
  "name": "kerryrandolph/wp-hook-annotations",
  "description": "Use PHP docblock @annotations to register WordPress hooks, filters and shortcodes",
  "keywords": ["wordpress", "annotations", "docblocks"],
  "type": "library",
  "homepage": "https://github.com/Kerry-Randolph/wp-hook-annotations",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Kerry Randolph",
      "email": "KerryWritesCode@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "php-di/php-di": "^6.0",
    "doctrine/cache": "^1.8",
    "doctrine/annotations": "^1.6",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "psr/log": "^1.1",
    "monolog/monolog": "^1.24",
    "squizlabs/php_codesniffer": "3.4.1",
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.1",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "HookAnnotations\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
